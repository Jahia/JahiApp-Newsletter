<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0" xmlns:sv="http://www.jcp.org/jcr/sv/1.0"
         xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
         xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:rep="internal">
    <templateSets jcr:primaryType="jnt:templateSets">
        <newsletter j:siteType="module" jcr:mixinTypes="jmix:accessControlled jmix:sitemap" jcr:primaryType="jnt:virtualsite"
              changefreq="monthly" j:installedModules="newsletter" priority="0.5">
            <home jcr:mixinTypes="jmix:sitemap" jcr:primaryType="jnt:page" changefreq="monthly"
                  j:templateNode="/templateSets/newsletter/templates/base/newsletters" priority="0.5"/>
            <newsletter-confirmation jcr:primaryType="jnt:page" j:templateNode="/templateSets/templates-intranet/templates/base/simple" >
              <j:translation_en jcr:lastModifiedBy="root" jcr:language="en" jcr:title="Newsletter confirmation" jcr:primaryType="jnt:translation" />
              <listA jcr:primaryType="jnt:contentList" >
                <bigText jcr:primaryType="jnt:bigText" >
                  <j:translation_en jcr:language="en" text="&lt;p&gt;  Your request has been taken into account.&lt;/p&gt; " jcr:primaryType="jnt:translation" />
                </bigText>
              </listA>
            </newsletter-confirmation>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <portlets jcr:primaryType="jnt:portletFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <base jcr:mixinTypes="jmix:renderable jmix:theme" jcr:primaryType="jnt:template" j:view="apps">
                    <newsletters jcr:primaryType="jnt:contentTemplate" j:applyOn="jnt:newsletters jnt:newsletter" j:defaultTemplate="true">
                      <pagecontent jcr:primaryType="jnt:contentList">
                        <mainResourceDisplay j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay" />
                      </pagecontent>
                    </newsletters>
                </base>
                <newsletter-issue jcr:mixinTypes="jmix:renderable" j:requiredPermissions="" jcr:primaryType="jnt:contentTemplate" j:view="newsletter" j:hiddenTemplate="false" j:applyOn="jnt:newsletterIssue" j:defaultTemplate="false">
                </newsletter-issue>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <files jcr:primaryType="jnt:folder"/>
            </templates>
            <newsletters jcr:primaryType="jnt:newsletters"/>
            <j:acl jcr:primaryType="jnt:acl">
                <GRANT_g_users j:privileges="jcr:read_live" j:protected="false" j:principal="g:users" j:aceType="GRANT"
                               jcr:primaryType="jnt:ace"/>
                <DENY_g_users j:privileges="jcr:read jcr:write_live jcr:modifyAccessControl jcr:write"
                              j:protected="false" j:principal="g:users" j:aceType="DENY" jcr:primaryType="jnt:ace"/>
                <GRANT_r_editor j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false"
                                j:principal="r:editor" j:aceType="GRANT" jcr:primaryType="jnt:ace"/>
                <DENY_r_editor j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false"
                               j:principal="r:editor" j:aceType="DENY" jcr:primaryType="jnt:ace"/>
                <GRANT_r_editor-in-chief
                        j:privileges="jcr:read jcr:write_live jcr:modifyAccessControl jcr:read_live jcr:write"
                        j:protected="false" j:principal="r:editor-in-chief" j:aceType="GRANT"
                        jcr:primaryType="jnt:ace"/>
                <GRANT_r_publisher j:privileges="jcr:read jcr:write_live jcr:read_live jcr:write" j:protected="false"
                                   j:principal="r:publisher" j:aceType="GRANT" jcr:primaryType="jnt:ace"/>
                <DENY_r_publisher j:privileges="jcr:modifyAccessControl" j:protected="false" j:principal="r:publisher"
                                  j:aceType="DENY" jcr:primaryType="jnt:ace"/>
                <GRANT_r_seo-manager j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false"
                                     j:principal="r:seo-manager" j:aceType="GRANT" jcr:primaryType="jnt:ace"/>
                <DENY_r_seo-manager j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false"
                                    j:principal="r:seo-manager" j:aceType="DENY" jcr:primaryType="jnt:ace"/>
                <GRANT_r_site-administrator
                        j:privileges="jcr:read jcr:write_live jcr:modifyAccessControl jcr:read_live jcr:write"
                        j:protected="false" j:principal="r:site-administrator" j:aceType="GRANT"
                        jcr:primaryType="jnt:ace"/>
                <GRANT_r_translator j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false"
                                    j:principal="r:translator" j:aceType="GRANT" jcr:primaryType="jnt:ace"/>
                <DENY_r_translator j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false"
                                   j:principal="r:translator" j:aceType="DENY" jcr:primaryType="jnt:ace"/>
                <GRANT_r_web-designer j:privileges="jcr:read jcr:read_live jcr:write" j:protected="false"
                                      j:principal="r:web-designer" j:aceType="GRANT" jcr:primaryType="jnt:ace"/>
                <DENY_r_web-designer j:privileges="jcr:write_live jcr:modifyAccessControl" j:protected="false"
                                     j:principal="r:web-designer" j:aceType="DENY" jcr:primaryType="jnt:ace"/>
            </j:acl>
        </newsletter>
    </templateSets>
</content>


